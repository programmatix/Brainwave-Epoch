{
  "name": "nw-react-example",
  "version": "0.1.3",
  "main": "main.js",
  "homepage": ".",
  "window": {
    "icon": "./build/logo512.png"
  },
  "node-remote": [
    "http://127.0.0.1:3042",
    "file://*"
  ],
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "nw": "0.83.0-sdk",
    "nw-builder": "^4.5.4",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-devtools": "^4.26.1",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^4.9.5",
    "wait-on": "^6.0.1",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "dev": "concurrently -k \"npm start\" \"wait-on http://127.0.0.1:3042 && cross-env NWJS_START_URL=http://127.0.0.1:3042 nw --enable-logging=stderr .\"",
    "dev-tools": "concurrently -k \"react-devtools\" \"cross-env REACT_APP_DEVTOOLS=enabled npm start\" \"wait-on http://127.0.0.1:3042 && cross-env NWJS_START_URL=http://127.0.0.1:3042 nw --enable-logging=stderr .\"",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "predist": "cross-env GENERATE_SOURCEMAP=false BUILD_PATH=./dist/app/build/ npm run build",
    "dist": "node dist.mjs"
  },
  "build": {
    "manifestProps": [
      "name",
      "version",
      "main",
      "node-remote",
      "window"
    ],
    "osTypes": [
      "windows"
    ]
  },
  "eslintConfig": {
    "globals": {
      "nw": true
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.4.4",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-crosshair": "^2.0.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-style": "^0.5.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "csv-parse": "^5.5.6",
    "react-chartjs-2": "^5.2.0"
  }
}
